<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>#</th>
      <th>Nome Completo</th>
      <th>Cpf</th>
      <th>Bloc</th>
      <th>Ap</th>
      <th>Permissao</th>
      <th>Adimplente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.user_name %></td>
        <td><%= user.pre_registration.cpf %></td>
        <td><%= user.pre_registration.bloc %></td>
        <td><%= user.pre_registration.ap %></td>
        <td><%= user.role %></td>
        <td><%= user.paid %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
        <td><%= link_to 'Destroy', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_admin_user_path %>
