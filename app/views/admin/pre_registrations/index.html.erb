<div class="container">

<p id="notice"><%= notice %></p>

<h1>Pre Registrations <%= link_to 'Novo(a)', new_admin_pre_registration_path, class:"btn btn-primary" %></h1>

<table class="table table-bordered table-hover">
  <thead class="thead-light">
    <tr>
      <th>#</th>
      <th>Cpf</th>
      <th>Bloc</th>
      <th>Ap</th>
      <th>Criado em</th>
      <th colspan="2">Ações</th>
    </tr>
  </thead>

  <tbody>
    <% @pre_registrations.each do |pre_registration| %>
      <tr>
        <td><%= pre_registration.id %></td>
        <td><%= pre_registration.cpf %></td>
        <td><%= pre_registration.bloc %></td>
        <td><%= pre_registration.ap %></td>
        <td><%= pre_registration.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_admin_pre_registration_path(pre_registration), class:"btn btn-primary" %>
          <%= link_to 'Destroy', admin_pre_registration_path(pre_registration), method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-danger" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>

